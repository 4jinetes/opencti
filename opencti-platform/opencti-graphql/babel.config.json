{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    ["@babel/transform-runtime"], ["@babel/transform-parameters", {
      "loose": true
    }]
  ],
  "env": {
    "development": {
      "sourceMaps": true,
      "retainLines": true,
    }
  },
}